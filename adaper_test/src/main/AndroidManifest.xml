<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pay.test">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.pay.test.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.pay.test.ListViewMultiItemActivity" />
        <activity android:name="com.pay.test.ListViewSectionOneItemActivity" />
        <activity android:name="com.pay.test.ListViewSectionMultiItemActivity"></activity>

        <activity
            android:name="com.pay.test.RecyclerViewActivity"
            android:label="one_item_recycler_view" />
        <activity
            android:name="com.pay.test.RecyclerViewMultiItemActivity"
            android:label="multi_item_recycler_view" />

        <activity
            android:name="com.pay.test.RecyclerViewSectionOneItemActivity"
            android:label="section_one_item_recycler_view" />

        <activity
            android:name="com.pay.test.RecyclerViewSectionMultiItemActivity"
            android:label="section_multi_item_recycler_view" />

        <activity
            android:name="com.pay.test.RecyclerViewHeaderAndFooterWrapperActivity"
            android:label="RecyclerViewHeaderAndFooterWrapperActivity" />

    </application>

</manifest>