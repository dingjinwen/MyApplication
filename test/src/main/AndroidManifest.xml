<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.test">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListViewMultiItemActivity" />
        <activity android:name=".ListViewSectionOneItemActivity" />
        <activity android:name=".ListViewSectionMultiItemActivity"></activity>

        <activity
            android:name=".RecyclerViewActivity"
            android:label="one_item_recycler_view" />
        <activity
            android:name=".RecyclerViewMultiItemActivity"
            android:label="multi_item_recycler_view" />

        <activity
            android:name=".RecyclerViewSectionOneItemActivity"
            android:label="section_one_item_recycler_view" />

        <activity
            android:name=".RecyclerViewSectionMultiItemActivity"
            android:label="section_multi_item_recycler_view" />

        <activity
            android:name=".RecyclerViewHeaderAndFooterWrapperActivity"
            android:label="RecyclerViewHeaderAndFooterWrapperActivity" />

    </application>

</manifest>